def reverse(self, x: int) -> int:
        
        revnum=0
        negative=x<0
        x=abs(x)
        while x>0:
            ld=x%10
            if revnum > (2**31 - 1) // 10 or (revnum == (2**31 - 1) // 10 and ld > 7):
                return 0
            revnum=(revnum*10)+ld
            x=x//10
        if negative:
            revnum = -revnum
    
        return revnum
